<div class= "container">

<h2> ADD NEW FARM</h2>
<%= form_for @farm, url: {action: "create"} do |f|%>
 <div class="form-group">
    <%= f.label :name%><br>
    <%= f.text_field :name%>
    <br>
    </div>
    <div class="form-group">
    <%= f.label :price%><br>
    <%= f.text_field :price%>RS
    <br>
    </div>
    <div class="form-group">
    <%= f.label :location%><br>
    <%= f.text_field :location%>
    <br>
    </div>
    <div class="form-group">
    <%= f.label :image%><br>
    <%= f.text_field :image%>
    <br>
    </div>
   
    <%= f.submit "Save"%>
<% end %>
</div>