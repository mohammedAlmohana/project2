<%= form_for @farm, url: {action: "create"} do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>
    <br>
    <%= f.label :price%>
    <%= f.text_field :price%>RS
    <br>
    <%= f.label :location%>
    <%= f.text_field :location%>
    <br>
    <%= f.label :image%>
    <%= f.text_field :image%>
    <br>
   
    <%= f.submit "Save"%>
<% end %>