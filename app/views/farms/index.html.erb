<% @farms.each do |farm|%>   
<tr>
<td><img src=<%=farm.image%> height="100" width="100"></td><br>

<td><%= link_to farm.name, farm_path(farm)%></td><br>
<td><%=farm.price%></td><br>
<td><%=farm.location%></td><br>
</tr>
<% end %>
  